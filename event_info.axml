              android:text="4" />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textColor="#000000"
              android:layout_weight="1"
              android:text="Старт" />
        </TableRow>
      </TableLayout>

    </android.support.v7.widget.CardView>

    <!--Итоговая таблица-->
    <android.support.v7.widget.CardView
        xmlns:cardview="http://schemas.android.com/apk/res-auto"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        cardview:cardCornerRadius="5dp"
        cardview:contentPadding="5dp"
        android:layout_marginBottom="6dp"
        android:background="#FFF000"
        android:layout_gravity="center_horizontal">

      <TextView
         android:text="  Итоговая таблица"
         android:textSize="14sp"
         android:layout_marginTop="5dp"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_alignParentRight="true"
         android:layout_alignParentEnd="true" />

      <Button
            style="?android:attr/borderlessButtonStyle"
            android:id="@+id/finaltable_open_button"
            android:textSize="15sp"
            android:layout_width="35px"
            android:layout_height="35px"
            android:layout_marginTop="5dp"
            android:layout_gravity="top|right"
            android:background="@drawable/ic_moreinfo" />

      <Button
            style="?android:attr/borderlessButtonStyle"
            android:id="@+id/finaltable_close_button"
            android:visibility="invisible"
            android:clickable="false"
            android:textSize="15sp"
            android:layout_width="35px"
            android:layout_height="35px"
            android:layout_marginTop="5dp"
            android:layout_gravity="top|right"
            android:background="@drawable/ic_lessinfo" />

      <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/final_table"
        android:visibility="gone"
        android:layout_marginTop="30dp">

        <TableRow
            android:background="#FFFAFA"
            android:padding="5dp">
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textColor="#000000"
              android:layout_weight="1"
              android:text="#" />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textColor="#000000"
              android:layout_weight="1"
              android:text="Команда" />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textColor="#000000"
              android:gravity="center"
              android:layout_weight="1"
              android:text="И" />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textColor="#000000"
              android:gravity="center"
              android:layout_weight="1"
              android:text="ГЗ" />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textColor="#000000"
              android:gravity="center"
              android:layout_weight="1"
